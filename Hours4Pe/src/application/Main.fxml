<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.MainController">
   <top>
      <HBox prefWidth="600.0" spacing="20.0" BorderPane.alignment="CENTER">
         <children>
      		<Label fx:id="userLbl" alignment="TOP_CENTER" text="Label" textAlignment="CENTER">
      			<font>
      				<Font name="Arial" size="18.0" />
      			</font>
      		</Label>
            <Button fx:id="addSchoolBut" contentDisplay="TOP" mnemonicParsing="false" text="Ajouter Ecole" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
      </HBox>
   </top>
</BorderPane>
